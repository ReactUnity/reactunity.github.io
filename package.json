{
  "name": "react-website",
  "version": "1.0.0",
  "private": true,
  "license": "CC",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "start": "next-remote-watch ./src/content",
    "build": "next build && node ./scripts/downloadFonts.js",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:source": "prettier --config .prettierrc --write \"{plugins,src}/**/*.{js,ts,jsx,tsx,css}\"",
    "nit:source": "prettier --config .prettierrc --list-different \"{plugins,src}/**/*.{js,ts,jsx,tsx,css}\"",
    "prettier": "npm run format:source",
    "prettier:diff": "npm run nit:source",
    "lint-heading-ids": "node scripts/headingIdLinter.js",
    "fix-headings": "node scripts/headingIdLinter.js --fix",
    "ci-check": "npm-run-all prettier:diff --parallel lint tsc lint-heading-ids",
    "tsc": "tsc --noEmit",
    "preview": "next start",
    "postinstall": "patch-package",
    "check-all": "npm-run-all prettier lint:fix tsc"
  },
  "dependencies": {
    "@babel/standalone": "^7.20.15",
    "@codesandbox/sandpack-react": "1.15.5",
    "@docsearch/css": "3.3.2",
    "@docsearch/react": "3.3.2",
    "@headlessui/react": "^1.7.9",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "debounce": "^1.2.1",
    "ga-lite": "2.1.4",
    "github-slugger": "^1.3.0",
    "next": "12.3.2-canary.7",
    "next-remote-watch": "^1.0.0",
    "parse-numeric-range": "^1.3.0",
    "react": "^18.2.0",
    "react-collapsed": "npm:@gaearon/react-collapsed@3.1.0-forked.1",
    "react-dom": "^18.2.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/mdx": "^2.2.1",
    "@types/babel__standalone": "^7.1.4",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/debounce": "^1.2.1",
    "@types/github-slugger": "^1.3.0",
    "@types/gtag.js": "^0.0.12",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.11.19",
    "@types/parse-numeric-range": "^0.0.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "asyncro": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "10.x",
    "eslint": "7.x",
    "eslint-config-next": "13.1.6",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "8.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.0",
    "globby": "^13.1.3",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": ">=13",
    "mdast-util-to-string": "^1.1.0",
    "metro-cache": "0.75.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.3",
    "reading-time": "^1.5.0",
    "remark": "^12.0.1",
    "remark-external-links": "^7.0.0",
    "remark-html": "^12.0.0",
    "remark-images": "^2.0.0",
    "remark-slug": "^7.0.0",
    "remark-unwrap-images": "^2.0.0",
    "retext": "^7.0.1",
    "retext-smartypants": "^4.0.0",
    "rss": "^1.2.2",
    "sass": "^1.58.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "unist-util-visit": "^2.0.3",
    "webpack-bundle-analyzer": "^4.7.0",
    "@lezer/javascript": "0.15.2"
  },
  "overrides": {
    "@lezer/javascript": "0.15.2"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 10,
    "showDetails": true
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css}": "npm run prettier",
    "src/**/*.md": "npm run fix-headings"
  }
}